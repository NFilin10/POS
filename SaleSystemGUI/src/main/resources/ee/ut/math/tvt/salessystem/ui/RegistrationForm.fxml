<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1">
<children>
    <TitledPane animated="false" collapsible="false" prefHeight="400.0" prefWidth="700" text="Log in">
        <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <children>
                    <HBox alignment="CENTER" layoutX="-1.0" prefHeight="377.0" prefWidth="700">
                        <VBox alignment="CENTER" prefHeight="389.0" prefWidth="293.0" spacing="4.0">
                            <children>
                                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="12.0" prefWidth="293.0" text="Sign up" textAlignment="CENTER">
                                    <font>
                                        <Font name="System Bold" size="20.0" />
                                    </font>
                                </Label>
                           <Label layoutX="13.0" layoutY="153.0" prefHeight="3.0" prefWidth="288.0" text="Name:">
                              <font>
                                 <Font size="13.0" />
                              </font>
                           </Label>
                                <TextField fx:id="name" />
                           <Label layoutX="10.0" layoutY="170.0" prefHeight="3.0" prefWidth="288.0" text="Email:">
                              <font>
                                 <Font size="13.0" />
                              </font>
                           </Label>
                           <TextField fx:id="name1" layoutX="10.0" layoutY="170.0" />
                           <HBox prefHeight="27.0" prefWidth="293.0">
                              <children>
                                      <Label prefHeight="3.0" prefWidth="288.0" text="Your role:">
                                          <font>
                                              <Font size="13.0" />
                                          </font>
                                      </Label>
                                      <ChoiceBox fx:id="role" prefHeight="25.0" prefWidth="286.0">
                                          <items>
                                              <FXCollections fx:factory="observableArrayList">
                                                  <String fx:value="Warehouse manager" />
                                                  <String fx:value="Cashier" />
                                                  <String fx:value="Customer" />
                                              </FXCollections>
                                          </items>
                                      </ChoiceBox>
                              </children>
                           </HBox>
                                <Label layoutX="10.0" layoutY="10.0" prefHeight="3.0" prefWidth="288.0" text="Username:">
                                    <font>
                                        <Font size="13.0" />
                                    </font>
                                </Label>
                                <TextField fx:id="username" />
                                <Label layoutX="10.0" layoutY="63.0" prefHeight="3.0" prefWidth="288.0" text="Password:">
                                    <font>
                                        <Font size="13.0" />
                                    </font>
                                </Label>
                                <PasswordField fx:id="password" />
                           <Label layoutX="10.0" layoutY="153.0" prefHeight="3.0" prefWidth="288.0" text="Confirm password:">
                              <font>
                                 <Font size="13.0" />
                              </font>
                           </Label>
                           <TextField fx:id="name11" layoutX="10.0" layoutY="343.0" />
                                <Button fx:id="reg" mnemonicParsing="false" onAction="#registerButtonAction" prefHeight="25.0" prefWidth="111.0" text="Sign up">
                                    <VBox.margin>
                                        <Insets top="12.0" />
                                    </VBox.margin></Button>
                            </children>
                            <opaqueInsets>
                                <Insets />
                            </opaqueInsets>
                        </VBox>
                    </HBox>

                </children>
            </AnchorPane>
        </content>
    </TitledPane>
</children>
</AnchorPane>
